    "express-jwt": "^5.3.1",
    "helmet": "^3.12.0",
    "jwks-rsa": "^1.2.1",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "CTTI",
    "PROPHETIC",
    "Open database",
    "node",
    "heroku",
    "create-react-app",
    "react"
  ]
